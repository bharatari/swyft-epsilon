{{partial "navbar-standard"}}

{{#animate-body}}
<div class="img-background container">
    <h1 class="background-title">faq</h1>
</div>

<div class="app-content">
    <div class="container">
        <ul class="faq">
            {{#each faq as |faqItem|}}
            <li>
                <p class="question">{{faqItem.question}}</p>
                <p class="answer">{{faqItem.answer}}</p>
            </li>
            {{/each}}
        </ul>
    </div>
</div>

{{/animate-body}}
{{partial "footer"}}