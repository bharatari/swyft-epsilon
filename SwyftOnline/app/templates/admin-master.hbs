</nav>

<div class="container">
    <table class="table table-hover master-sheet">
        <tr>
            <th>Name</th>
            <th>Items</th>
            <th>Total</th>
            <th>Location</th>
            <th>Phone</th>
            <th>Payment</th>
            <th>Charge</th>
        </tr>
        {{#each masterItem in model.items}}
        <tr>
            <td>{{masterItem.name}}</td>
            <td>
                <ul>
                    {{#each item in masterItem.items}}
                    <li>
                        <p>{{item.quantity}} <strong>{{item.name}}</strong> {{item.itemComments}}</p>
                    </li>
                    {{/each}}
                </ul>
            </td>
            <td>{{render-decimals masterItem.total}}</td>
            <td>{{masterItem.deliveryLocation}}</td>
            <td>{{masterItem.phoneNumber}}</td>
            <td>{{masterItem.paymentType}}</td>
            <td>{{masterItem.charge}}</td>
        </tr>
        {{/each}}
    </table>
    <h3 class="master-sheet master-sheet-total">Total ${{model.deliveryTotal}}</h3>
</div>
