{{#if property.editable}}
    {{#if typeString}}
        {{#if property.largeField}}
            <div class="form-group admin-control-group">
                <label class="col-sm-2 control-label admin-control-label">{{property.displayName}}</label>
                <div class="col-sm-9">
                    {{textarea class=(concat 'text-area ' adminDiscourage) value=property.value}}
                </div>
            </div>
        {{else}}
            <div class="form-group admin-control-group">
                <label class="col-sm-2 control-label admin-control-label">{{property.displayName}}</label>
                <div class="col-sm-9">
                    {{input class=adminDiscourage value=property.value type="text"}}
                </div>
            </div>
        {{/if}}
    {{/if}}

    {{#if typeNumber}}
         <div class="form-group admin-control-group">
            <label class="col-sm-2 control-label admin-control-label">{{property.displayName}}</label>
            <div class="col-sm-9">
                {{input class=adminDiscourage value=property.value type="number"}}
            </div>
        </div>
    {{/if}}
    
    {{#if typeDateTime}}
        <div class="form-group admin-control-group">
            <label class="col-sm-2 control-label admin-control-label">{{property.displayName}}</label>
            <div class="col-sm-9">
                {{date-time-picker class=adminDiscourage initialValue=property.value action=(action (mut property.value))}}
            </div>
        </div>
    {{/if}}

    {{#if typeEnum}}
        <div class="form-group admin-control-group">
            <label class="col-sm-2 control-label admin-control-label">{{property.displayName}}</label>
            <div class="col-sm-9">
                {{select-menu action=(action (mut property.value)) content=property.enum selection=property.value optionValuePath="propertyName" optionLabelPath="displayName" class=adminDiscourage prompt="Select a Value"}}
            </div>
        </div>    
    {{/if}}

    {{#if typeBoolean}}
        <div class="form-group admin-control-group">
            <label class="col-sm-2 control-label admin-control-label">{{property.displayName}}</label>
            <div class="col-sm-9">
                {{select-menu action=(action (mut property.value)) content=booleanValues selection=property.value optionValuePath="propertyName" optionLabelPath="displayName" class=adminDiscourage prompt="Select a Value"}}
            </div>
        </div>  
    {{/if}}
    
    {{#if typeArray}}
        <div class="row">
            <div class="col-md-12 col-md-offset-2">
                <h2 class="menu-item-wizard-header">{{property.displayName}}</h2>
                Editing data through data tree not supported
                {{data-tree data=property.value rootName="" editable=false}}
            </div>
        </div>
    {{/if}}
    
    {{#if typeObject}}
        <div class="row">
            <div class="col-md-12 col-md-offset-2">
                <h2 class="menu-item-wizard-header">{{property.displayName}}</h2>
                Editing data through data tree not supported
                {{data-tree data=property.value rootName="" editable=false}}
            </div>
        </div>
    {{/if}}
{{else}}
    {{#if typeString}}
        {{#if property.largeField}}
            <div class="form-group admin-control-group">
                <label class="col-sm-2 control-label admin-control-label">{{property.displayName}}</label>
                <div class="col-sm-9">
                    {{textarea class=(concat 'text-area ' adminDiscourage) value=property.value readonly="true"}}
                </div>
            </div>
        {{else}}
            <div class="form-group admin-control-group">
                <label class="col-sm-2 control-label admin-control-label">{{property.displayName}}</label>
                <div class="col-sm-9">
                    {{input class=adminDiscourage value=property.value type="text" readonly="true"}}
                </div>
            </div>
        {{/if}}
    {{/if}}

    {{#if typeNumber}}
         <div class="form-group admin-control-group">
            <label class="col-sm-2 control-label admin-control-label">{{property.displayName}}</label>
            <div class="col-sm-9">
                {{input class=adminDiscourage value=property.value type="number" readonly="true"}}
            </div>
        </div>
    {{/if}}
    
    {{#if typeDateTime}}
        <div class="form-group admin-control-group">
            <label class="col-sm-2 control-label admin-control-label">{{property.displayName}}</label>
            <div class="col-sm-9">
                {{date-time-picker class=adminDiscourage initialValue=property.value action=(action (mut property.value)) readonly="true"}}
            </div>
        </div>
    {{/if}}

    {{#if typeEnum}}
        <div class="form-group admin-control-group">
            <label class="col-sm-2 control-label admin-control-label">{{property.displayName}}</label>
            <div class="col-sm-9">
                {{select-menu action=(action (mut property.value)) content=property.enum selection=property.value optionValuePath="propertyName" optionLabelPath="displayName" class=adminDiscourage disabled="true" prompt="No Value"}}
            </div>
        </div>    
    {{/if}}

    {{#if typeBoolean}}
        <div class="form-group admin-control-group">
            <label class="col-sm-2 control-label admin-control-label">{{property.displayName}}</label>
            <div class="col-sm-9">
                {{select-menu action=(action (mut property.value)) content=booleanValues selection=property.value optionValuePath="propertyName" optionLabelPath="displayName" class=adminDiscourage disabled="true" prompt="No Value"}}
            </div>
        </div>  
    {{/if}}
    
    {{#if typeArray}}
        <div class="row">
            <div class="col-md-12 col-md-offset-2">
                <h2 class="menu-item-wizard-header">{{property.displayName}}</h2>
                {{data-tree data=property.value rootName="" editable=false}}
            </div>
        </div>
    {{/if}}
    
    {{#if typeObject}}
        <div class="row">
            <div class="col-md-12 col-md-offset-2">
                <h2 class="menu-item-wizard-header">{{property.displayName}}</h2>
                {{data-tree data=property.value rootName="" editable=false}}
            </div>
        </div>
    {{/if}}
{{/if}}
