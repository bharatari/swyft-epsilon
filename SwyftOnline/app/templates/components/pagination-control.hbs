<nav>
    <ul class="pagination">
        {{#unless onFirstPage}}
            <li><a href="#" {{action 'goToFirstPage'}}>...</a></li>
        {{/unless}}
        {{#each page in previousFive}}
            <li><a href="#" {{action 'goToPage' page}}>{{page}}</a></li>
        {{/each}}
        <li><a href="javascript:void(0)">{{currentPage}}</a></li>
        {{#each page in nextFive}} 
            <li><a href="#" {{action 'goToPage' page}}>{{page}}</a></li>
        {{/each}}
        {{#unless onLastPage}}
            <li><a href="#" {{action 'goToLastPage'}}>...</a></li>
        {{/unless}}
    </ul>
    <p>Page {{currentPage}} of {{totalPages}}</p>
</nav>